extends layout

block content
 div#map(style='width: 900px; height: 800px;')
 for item in result
  div.location(style='display: none;')
   p=item.location_name 
   p=item.lat
   p=item.lng
   p=item.price_formatted
   p=item.available_spots
 script(src='http://maps.google.com/maps/api/js?sensor=false')
  
 script(src='http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.10.1.min.js')

 script(src="javascripts/google.js")
